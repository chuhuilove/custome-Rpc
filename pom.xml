    <properties>
        <netty.version>4.1.40.cyzi-custome</netty.version>
        <junitTest.version>4.12</junitTest.version>
        <hamcrest.version>1.3</hamcrest.version>
        <custome-rpc.version>1.0-SNAPSHOT</custome-rpc.version>
        <commons-collections.version>4.4</commons-collections.version>
        <commons-io.version>2.6</commons-io.version>
